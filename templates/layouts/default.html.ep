<!DOCTYPE html>
<html lang="en">
  <head>
  
    %= include 'layouts/_headers'
    
    <!-- OTHERS -->
    <title>PERLogger v<%= config->{version} %></title>
  </head>
  <body>
    <div id="app">
      <div class="container-fluid px-0">
       %= include 'layouts/_page_head' 
        <div class="row flex-xl-nowrap2 mx-0">
          %= include 'layouts/_page_left_menu'
          <div class="bd-content col-md-9 col-12" style="background: #28a7451f;">
            <%= content %>
          </div>
        </div>
      </div>
    </div>
    
%= javascript begin

    var app = new Vue({
      el: '#app',
      data: {
                counter: 0,
                stopit: false,
                message: 'Hello Vue.js!'
            }
      })

% end  

  </body>
</html>
