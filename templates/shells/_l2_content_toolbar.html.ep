<div class="sticky">
  <b-button-group>
    <b-button size="sm" variant="primary" disabled v-on:click="runBatch()"
      :disabled="shells.disableShellRunBatchButton">
      <b-icon icon="collection-play"></b-icon>
    </b-button>
  </b-button-group>
  <b-button-group v-if="shells.l2_output.length > 0">
    <b-button size="sm" variant="success" v-on:click="shells.l2_output = []">
      <b-icon icon="clipboard-x"></b-icon>
    </b-button>
  </b-button-group>
</div>